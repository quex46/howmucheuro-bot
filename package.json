{
  "name": "howmucheuro-bot",
  "description": "Currency converting bot for my fellas",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "cdk": "node -r dotenv/config node_modules/.bin/cdk",
    "clean": "rimraf cdk.out",
    "build": "yarn cdk synth -q",
    "deploy": "yarn cdk deploy --all --require-approval=never",
    "lint": "eslint src",
    "test": "jest --passWithNoTests",
    "webhook": "npx ts-node -r dotenv/config src/set-webhook.ts"
  },
  "devDependencies": {
    "@types/jest": "28.1.6",
    "@types/node": "16.11.7",
    "@types/node-fetch": "2.6.2",
    "@types/numeral": "2.0.2",
    "@types/source-map-support": "0.5.4",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "aws-cdk": "2.36.0",
    "dotenv": "16.0.1",
    "esbuild": "0.14.54",
    "eslint": "8.21.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "jest": "28.1.3",
    "rimraf": "3.0.2",
    "ts-jest": "28.0.7",
    "ts-node": "10.9.1",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "aws-cdk-lib": "2.36.0",
    "constructs": "10.1.117",
    "node-fetch": "2.6.6",
    "numeral": "2.0.6",
    "source-map-support": "0.5.21"
  }
}
